  <ItemGroup>
    <None Include="wwwroot\data\MLModelBuildermbconfig.zip">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.ML" Version="1.6.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="wwwroot\data\companies.json" />
    <Resource Include="wwwroot\data\educations.json" />
    <Resource Include="wwwroot\data\locations.json" />
